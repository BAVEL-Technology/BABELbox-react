{
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "env": {
      "es6": true,
      "browser": true,
      "commonjs": true,
      "node": true
    },
    "extends": "prettier",
    "plugins": ["prettier"],
    "rules": {
      "camelcase": "warn",
      "no-var": "warn",
      "prefer-const": "warn",
      "quotes": ["warn", "single"],
      "semi": ["warn", "always"],
      "yoda": "error"
    }
  }
